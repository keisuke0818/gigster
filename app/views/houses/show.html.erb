
    <div id="fb-root"></div>
    <script>(function(d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) return;
      js = d.createElement(s); js.id = id;
      js.src = "//connect.facebook.net/ja_JP/all.js#xfbml=1&appId=153617311511977";
      fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
    </script>
    <div id="fb-root"></div>

    <div class="container">

      <!-- Typography
      ================================================== -->
      <div class="bs-docs-section">
        <div class="row">
          <div class="col-lg-12">
            <div class="page-header">
              <h2 id="type"><%= @house.name %></h2>
            </div>
          </div>
        </div>
        <%= @house.day %>(<%= @house.city %> , <%= @house.country %>)　
        <% if user_signed_in? && current_user.id == @house.users_ids %>
          <%= link_to '(編集)', edit_house_path(@house), :style => "color:18bc9c;" %>
        <% end %>

        <script type="text/javascript" language="javascript">
        document.write('<div class="fb-like" data-href="http://gigster.jp/gigs/<%= @house.id %>" data-width="450" data-layout="button_count" data-show-faces="true" data-send="true"></div>');
        </script>

        <br/><br/>

        <%= image_tag @house.image_url, :size => "500x300" %>

        <!-- Headings -->

        <div class="row">
          <div class="col-lg-4">

            <div class="bs-example">
              <p class="lead"><%= br(@house.detail) %></p>
            </div>
            <br/><br/>

            <% if @house.googlemap_url != "" %>

            <% end %>

            <div class="fb-comments" data-href="<%=@meta_og_url%>" data-width="400"></div>

          </div>
        </div>

        <br/><br/>

        <%= link_to '一覧へ戻る', '/houses' %>
        <br/><br/>

    </div>



